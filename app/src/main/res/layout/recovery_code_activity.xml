<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutDirection="rtl"
    tools:context="com.app.leon.moshtarak.Activities.SessionActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_margin="@dimen/activity_mid_margin"
        android:gravity="left"
        android:orientation="vertical"
        tools:ignore="MissingConstraints,RtlHardcoded">

        <EditText
            android:id="@+id/editTextSearch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/activity_mid_margin"
            android:background="@drawable/border_gray_2"
            android:hint="@string/search"
            android:padding="@dimen/activity_mid_padding"
            tools:ignore="Autofill,TextFields" />

        <ListView
            android:id="@+id/listViewRequest"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/activity_mid_margin"
            android:divider="@null"
            android:dividerHeight="0dp" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
