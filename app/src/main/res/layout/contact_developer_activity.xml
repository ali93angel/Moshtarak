<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.app.leon.moshtarak.Activities.ContactDeveloperActivity">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="440dp"
        app:srcCompat="@drawable/programmer_"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-10dp" />

    <!--    android:layout_marginTop="200dp"-->
    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginTop="345dp"
        android:foregroundGravity="center"
        app:srcCompat="@drawable/img_profile_"
        tools:ignore="ContentDescription,MissingConstraints" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/imageView2"
        android:layout_marginTop="200dp"
        android:gravity="center"
        android:text="@string/programmer_team"
        android:textSize="@dimen/textSizeLarge_" />
</RelativeLayout>
